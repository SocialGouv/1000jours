{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "start-clear": "expo start --clear",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest",
    "test-watch": "jest --watchAll",
    "lint": "eslint .",
    "lint-fix": "eslint --fix .",
    "publish-develop": "eas update --auto --non-interactive",
    "publish-preprod": "eas update --channel preprod --message 'EAS Update - preprod'",
    "build-prod:android": "eas build --platform android --profile production --non-interactive --clear-cache",
    "build-local-prod:android": "ORG_GRADLE_JVMARGS='-Xmx4g -XX:MaxMetaspaceSize=1g' NODE_ENV=production eas build --platform android --profile production --local --clear-cache",
    "build-local-prod:ios": "NODE_ENV=production eas build --platform ios --profile production --local --non-interactive --clear-cache",
    "build-prod:ios": "eas build --platform ios --non-interactive --clear-cache",
    "upload:android": "expo upload:android --key ./api-key-play-store.json --latest --track internal",
    "upload:ios": "fastlane deliver --ipa app.ipa --api_key_path ./api-key-app-store.json --force true --fix-ios-deploy false",
    "publish-prod": "yarn upload:android && yarn upload:ios",
    "postinstall": "patch-package"
  },
  "jest": {
    "preset": "jest-expo",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transformIgnorePatterns": [
      "./node_modules/(?!native-base)/"
    ],
    "setupFiles": [
      "./src/jest/setup.js"
    ],
    "collectCoverage": true
  },
