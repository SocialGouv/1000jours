{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "start-clear": "expo start --clear",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest --watchAll",
    "lint": "eslint .",
    "lint-fix": "eslint --fix .",
    "build-staging:android": "expo build:android --release-channel staging",
    "build-staging:ios": "expo build:ios --release-channel staging",
    "build-dev": "yarn build-staging:ios && yarn build-staging:android",
    "publish-dev": "expo publish --release-channel staging",
    "build-prod:android": "expo build:android --release-channel prod",
    "build-prod:ios": "expo build:ios --release-channel prod",
    "build-prod": "yarn build-prod:ios && yarn build-prod:android",
    "upload:android": "expo upload:android --key ./api-key-play-store.json --latest --track beta",
    "upload:ios": "fastlane deliver --ipa app.ipa --api_key_path ./api-key-app-store.json --force true",
    "publish-prod": "yarn upload:android && yarn upload:ios"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
