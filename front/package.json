{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "start-clear": "expo start --clear",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest",
    "test-watch": "jest --watchAll",
    "lint": "eslint .",
    "lint-fix": "eslint --fix .",
    "publish-develop": "expo publish --release-channel develop",
    "publish-preprod": "expo publish --release-channel preprod",
    "build-prod:android": "expo build:android --release-channel prod",
    "build-prod:ios": "expo build:ios --release-channel prod",
    "upload:android": "expo upload:android --key ./api-key-play-store.json --latest --track internal",
    "upload:ios": "fastlane deliver --ipa app.ipa --api_key_path ./api-key-app-store.json --force true",
    "publish-prod": "yarn upload:android && yarn upload:ios"
  },
  "jest": {
    "preset": "jest-expo",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transformIgnorePatterns": [
      "./node_modules/(?!native-base)/"
    ],
    "setupFiles": [
      "./src/jest/setup.js"
    ],
    "collectCoverage": true
  },
